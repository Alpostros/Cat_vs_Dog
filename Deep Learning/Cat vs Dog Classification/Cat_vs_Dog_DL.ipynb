      "source": [
        "from zipfile import ZipFile\n",
        "with ZipFile('train.zip', 'r') as zipObj:\n",
        "   zipObj.extractall()\n",
        "\n",
        "with ZipFile('test.zip', 'r') as zipObj:\n",
        "   zipObj.extractall()"
      ],
